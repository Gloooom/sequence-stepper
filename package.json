{
  "name": "sequence-stepper",
  "version": "0.5.2",
  "description": "The small lib for the asynchronous control of queue of functions",
  "main": "build/stepper.js",
  "scripts": {
    "test": "ava tests.js",
    "transpile": "babel ./src --out-dir ./build",
    "prepublish": "npm run transpile"
  },
  "keywords": [
    "control",
    "stack",
    "queue",
    "stepper",
    "function",
    "async",
    "asynchronous",
    "sequence",
    "steps"
  ],
  "bugs": {
    "email": "pyatyispyatil@gmail.com",
    "url": "https://github.com/gloooom/sequence-stepper/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gloooom/sequence-stepper/"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "standard": {
    "ignore": [
      "/node_modules/",
      "/build/",
      "webpack.config.js"
    ],
    "parser": "babel-eslint"
  },
  "author": "Nikolay Ryabov",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0"
  }
}
